;; *** UPPER CASE ALIASES for CAPS layer ***
(defalias
  .C‚åò (switch
        (rsft lsft) (unshift c) break ;; Shift on caps-layer become lowercase again
        (rctl lctl) (unshift c) break ;; do Ctrl+C instead of Ctrl+Shift+C
        ()          (tap-hold 1 $200 S-c @.`) break
