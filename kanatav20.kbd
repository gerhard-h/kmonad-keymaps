(deftemplate same-side-lshift (defaultaction trigger action)
	(switch
		(rsft lctl rctl ralt lalt lmet rmet) $defaultaction break ;; lshift is missing from the list 
		($trigger) $action break
		() $defaultaction break
	)
)
