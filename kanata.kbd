;; FAKE KEYS
 ;; (defalias
 ;;   lft (multi (release-key ralt) left) ;; release ralt if held and also press left
 ;;   rgt (multi (release-key ralt) rght) ;; release ralt if held and also press rght
 ;;   rlr (release-layer ralted)          ;; release layer-toggle of ralted
